------反绕过速度检测local Players = game:GetService("Players")local LocalPlayer = Players.LocalPlayerlocal Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()local Humanoid = Character:WaitForChild("Humanoid")local FakeSpeexSBFakeSpeexSB = hookmetamethod(game, "__index", function(self, key)    if self == Humanoid and key == "WalkSpeed" then        return 16    end    return FakeSpeexSB(self, key)end)------绕过踢出 传送 local Players = game:GetService("Players")local LocalPlayer = Players.LocalPlayerlocal TeleportService = game:GetService("TeleportService")local oldKickFunctionif hookfunction then    oldKickFunction = hookfunction(LocalPlayer.Kick, function() end)endlocal oldHmmi = hookmetamethod(game, "__index", function(self, method)    local methodLower = method:lower()        if self == LocalPlayer and methodLower == "kick" then        return error("应用 ':' 而非 '.' 调用成员函数 Kick", 2)    end        if self == TeleportService then        if methodLower == "teleport" or method == "TeleportToPlaceInstance" then            return error("应用 ':' 而非 '.' 调用成员函数 "..method, 2)        end    end        return oldHmmi(self, method)end)local oldHmmnc = hookmetamethod(game, "__namecall", function(self, ...)    local method = getnamecallmethod()    local methodLower = method:lower()        if self == LocalPlayer and methodLower == "kick" then        return    end        if self == TeleportService and (methodLower == "teleport" or method == "TeleportToPlaceInstance") then        return    end        return oldHmmnc(self, ...)end)-----反甩飞local Players = game:GetService("Players")local function bypassfling(speaker)    for _, player in pairs(Players:GetPlayers()) do        if player ~= speaker and player.Character then            for _, part in pairs(player.Character:GetDescendants()) do                if part:IsA("BasePart") then                    part.CanCollide = false                end            end        end    endendbypassfling(Players.LocalPlayer)------snow公开 基本普通功能 <来源iy指令>
